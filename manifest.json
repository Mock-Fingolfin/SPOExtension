{
  "name": "SPO New Tab",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Strange Potato Owls Replace New Tab",
  "homepage_url": "https://strangepotatoowls.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action":{
    "default_title":"SPO New Tab",
    "default_popup":"popup.html"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "override.html"
  },
  "options_page": "options.html",
  "permissions": ["storage"]

  
}
